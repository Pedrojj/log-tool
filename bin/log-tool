#!/bin/bash

PWD="`pwd`"
WDir=`basename $PWD`
#echo $WDir

# Script must be run from the project root directory or the ./bin directory
if [ $WDir = "bin" ]
then
  cd ..
elif [ $WDir != "su-log-tool" ]
then
  echo "Unknown directory"
fi

#just going to assume some common/useful/sane default arguments.
node logtool.js
